<!-- Puppet managed file -->
<config>
<fold-anon v='false'/>
<cause v='<%= @cause %>'/>
<power v='<%= @power %>'/>
<bigpackets v='<%= @bigpackets %>'/>
<passkey v='<%= @passkey %>'/>
<user v='<%= @user %>'/>
<team v='<%= @team_id %>'/>
<gpu v='<%= scope.call_function('bool2str', [@gpu]) %>'/>
<slot id='0' type='CPU'/>
<%-
i = 0
while i < @cpu_slots do
%>
  <slot id='<%= slot %>' type='CPU'>
    <cpus v='<%= @cpus_per_slot %>'/>
  </slot>
<%-
  i += 1
end
if @gpu
  i = @cpu_slots
  while i < @gpu_slots do
    slot = i + 1
%>
<slot id='<%= slot %>' type='GPU'/>
<%
    i += 1
  end
end -%>
</config>
